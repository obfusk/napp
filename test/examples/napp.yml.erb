napp:
  dirs:
    apps: <%= dir_apps %>
    log: <%= dir_log(:abs) %>
    nginx: nginx.d
    app:
      app: app
      cfg: cfg
      log: log
      run: run
  user: napp
  users: []
  log_w_sudo: true
defaults:
  app:
    vcs: git
    branch: master
  types:
    daemon:
      logdir: log
      public: public
  daemon:
    wait: 7
  nginx:
    validate_server_name: true
    max_body_size:
    proxy_buffering:
    ssl: false
commands:
  aliases:
    BUNDLE: bundle install
    RACKUP: SIGINT bundle exec rackup -E production -p ${PORT}
  mongo:
  postgres:
